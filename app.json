{
  "expo": {
    "name": "repify",
    "slug": "repify",
    "version": "1.0.0",
    "orientation": "portrait",

    "icon": "./assets/images/icon.png",

    "scheme": "repify",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "ios": {
      "supportsTablet": true
    },

    "android": {
      "package": "com.repify.app.v2",

      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },

    "plugins": [
      "expo-router"
    ],

    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },

    "extra": {
      "router": {},
      "eas": {
        "projectId": "947ef241-5252-4946-a13a-10129d1a373b"
      }
    }
  }
}
